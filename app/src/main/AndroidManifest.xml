<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.librapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Librapp">
        <activity android:name=".AdminSectionActivity"></activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".FreeAccessActivity" />
        <activity android:name=".UserLoggedActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>